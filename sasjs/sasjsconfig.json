{
  "$schema": "https://cli.sasjs.io/sasjsconfig-schema.json",
  "macroFolders": [
    "sasjs/macros"
  ],
  "programFolders": [
    "sasjs/MONITORBD"
  ],
  "serviceConfig": {
    "serviceFolders": [
      "sasjs/services/common",
      "sasjs/services/files"
    ],
    "initProgram": "sasjs/init.sas"
  },
  "jobConfig": {
    "jobFolders": [
      "sasjs/jobs/admin"
    ],
    "initProgram": "sasjs/init.sas"
  },
  "defaultTarget": "Monitor",
  "targets": [
    {
      "name": "server",
      "syncFolder": "sasjs/mocks",
      "serverUrl": "http://localhost:5000",
      "serverType": "SASJS",
      "appLoc": "/Monitor/sas9",
      "streamConfig": {
        "streamLogo": "assets/img/logo.jpg",
        "streamServiceName": "Monitor",
        "streamWeb": true,
        "streamWebFolder": "web",
        "webSourcePath": "dist/BE"
      }
    },
    {
      "name": "Monitor",
      "serverUrl": "http://clsmv3sas010.cencosud.corp:7980",
      "serverType": "SAS9",
      "httpsAgentOptions": {
        "allowInsecureRequests": false
      },
      "appLoc": "/Cencosud/Monitor/sas9",
      "deployConfig": {
        "deployServicePack": true,
        "deployScripts": []
      },
      "serverName": "SASApp",
      "repositoryName": "Foundation"
    }
  ]
}